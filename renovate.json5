{
  "extends": [
    "config:base"
  ],
  "enabledManagers": [
    "dockerfile"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "enabled": false,
    },
    {
      "enabled": true,
      "groupName": "DBRE packages",
      "matchManagers": [
        "dockerfile"
      ],
      "paths": [
        "folder-to-include/**",
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ]
}
